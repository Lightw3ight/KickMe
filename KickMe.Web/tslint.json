{
	"rules": {
		"class-name": true,
		"curly": true,
		"eofline": true,
		"for-in": true,
		"interface-name": true,
		"no-any": false,
		"no-arg": true,
		"no-bitwise": true,
		"no-consecutive-blank-lines": true,
		"no-construct": true,
		"no-duplicate-key": true,
		"no-duplicate-variable": true,
		"no-empty": true,
		"no-eval": true,
		"no-for-in-on-array": true,
		"no-string-literal": true,
		"no-switch-case-fall-through": true,
		"no-trailing-comma": true,
		"no-trailing-whitespace": true,
		"no-unused-variables": [ true, "check-paramaters" ],
		"no-unused-expression": true,
		"no-unreachable": true,
		"no-use-before-declare": true,
		"one-line": [
			true,
			"check-open-brace",
			"check-catch",
			"check-else",
			"check-whitespace"
		],
		"quotemark": [ true, "single" ],
		"radix": false,
		"semicolon": true,
		"triple-equals": [ true, "allow-null-check" ],
		"type-def": [ false, "call-signature", "index-signature", "parameter", "property-signature", "variable-declarator", "member-variable-declarator" ],
		"typedef-whitespace": [ true, "call-signature" ],
		"use-strict": [
			true,
			"check-module",
			"check-function"
		],
		"variable-name": [ false, "allow-leading-underscore" ]
	}
}
